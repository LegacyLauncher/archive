{
  "revision": 5,
  "required": 1.79,
  "variables": {
    "skip-folders": "millenaire",
    "heap-pattern": "(?:Could not reserve enough space for (?:[0-9]+KB |)object heap)$",
    "oom-pattern": "^java.lang.OutOfMemoryError: .+",
    "forge-pattern": ".*(?i:forge).*"
  },
  "buttons": [
    {
      "text": "disable-optimized",
      "actions": [
        "set --minecraft.improvedargs false"
      ],
      "block": true
    },
    {
      "text": "open-settings",
      "actions": [
        "gui settings",
        "exit"
      ]
    },
    {
      "text": "fix-heap",
      "actions": [
        "set --minecraft.memory fix",
        "exit"
      ]
    },
    {
      "text": "install-java64",
      "actions": [
        "browse --www http://tlaun.ch/wiki/update:java"
      ]
    },
    {
      "text": "logs",
      "actions": [
        "gui logs"
      ]
    }
  ],
  "signatures": [
    {
      "name": "direct-close",
      "pattern": "Someone is closing me!",
      "version": ".*1\\.5\\.2$",
      "fake": true
    },
    {
      "name": "incorrent-mods-version",
      "pattern": ".*java\\.lang\\.NoClassDefFoundError\\: net/minecraftforge/fml/relauncher/IFMLLoadingPlugin$",
      "version": "${forge-pattern}"
    },
    {
      "name": "codechicken-core-missing",
      "pattern": ".*java\\.lang\\.NoClassDefFoundError: codechicken/.*",
      "version": "${forge-pattern}",
      "buttons": [
        {
          "text": "download",
          "actions": [
            "browse --www http://mods.curse.com/mc-mods/minecraft/222213-codechickencore#t1:other-downloads"
          ]
        }
      ]
    },
    {
      "name": "permgen",
      "exitCode": -805306369,
      "buttons": [
        {
          "text": "set-permgen",
          "actions": [
            "set --minecraft.improvedargs false",
            "set --minecraft.javaargs -XX:MaxPermSize=256M"
          ],
          "block": true
        }
      ],
      "jre": "1\\.[0-7]\\..+",
      "_jre": "up to 1.7"
    },
    {
      "name": "metaspace",
      "exitCode": -805306369,
      "buttons": [
        "open-settings"
      ]
    },
    {
      "name": "oom-x86",
      "pattern": "${oom-pattern}",
      "archIssue": true,
      "buttons": [
        "install-java64"
      ]
    },
    {
      "name": "oom",
      "pattern": "${oom-pattern}",
      "buttons": [
        "disable-optimized",
        "open-settings"
      ]
    },
    {
      "name": "invalid-username",
      "pattern": "^[\\s]*at org.apache.logging.log4j.util.PropertiesUtil.<init>\\(PropertiesUtil.java:[0-9]+\\)$",
      "os": [
        "windows"
      ],
      "buttons": [
        "open-settings"
      ]
    },
    {
      "name": "firewall-block",
      "pattern": "^Caused by: io\\.netty\\.channel\\.ChannelException: failed to open a new selector$",
      "os": [
        "windows"
      ]
    },
    {
      "name": "server-resource-pack",
      "pattern": "^(.*)\\[Downloader 0\\/ERROR]: INTERRUPTED$"
    },
    {
      "name": "heap-x86",
      "pattern": "${heap-pattern}",
      "archIssue": true,
      "buttons": [
        "install-java64"
      ]
    },
    {
      "name": "heap",
      "pattern": "${heap-pattern}",
      "buttons": [
        "fix-heap",
        "open-settings"
      ]
    },
    {
      "name": "arch",
      "archIssue": true,
      "buttons": [
        "install-java64"
      ]
    },
    {
      "name": "extracted-mods-found",
      "pattern": "^java\\.lang\\.RuntimeException: Extracted mod jars found, loading will NOT continue$",
      "version": "${forge-pattern}"
    }
  ]
}