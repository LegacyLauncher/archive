{
  variables: {
    "forge": "^(?:[0-9-]+ [0-9:]+ \\[[\\w]+\\]\\ {0,1}\\[{0,1}[\\w]*\\]{0,1}\\ {0,1}){0,1}",
    "crash": "^.*[\\#\\@\\!\\@\\#][ ]Game[ ]crashed!.+[\\#\\@\\!\\@\\#][ ](.+)$"
  },
  signatures: [
    {
      name: "Bad video drivers",
      path: "opengl",
      pattern: '[\\s]*org\\.lwjgl\\.LWJGLException\\: Pixel format not accelerated',
      forge: true
    },
    {
      name: "Invalid META-INF",
      path: "meta-inf",
      exit: 1,
      pattern: 'Exception in thread "main" java.lang.SecurityException: SHA1 digest error for .+',
      forge: true
    },
    {
      name: "Direct close bug",
      exit: 1,
      pattern: 'Someone is closing me!',
      forge: true,
      fake: true
    },
    {
      name: "Missing main class",
      path: "missing-main",
      exit: 1,
      pattern: '^Error: Could not find or load main class .+'
    },
    {
      name: "PermGen error",
      path: "permgen-error",
      exit: -805306369
    },
    {
      name: "OutOfMemory error",
      path: "outofmemory",
      exit: -1,
      pattern: '^java.lang.OutOfMemoryError: .+'
    },
    {
      name: "Invalid system user name",
      path: "invalid-sys-username",
      exit: 1,
      pattern: "^[\\s]*at org.apache.logging.log4j.util.PropertiesUtil.<init>\\(PropertiesUtil.java:71\\)$"
    },
    {
      name: "Firewall error",
      path: "firewall",
      exit: -1,
      pattern: '^Caused by: io\\.netty\\.channel\\.ChannelException: failed to open a new selector$'
    },
    {
      name: "Bad ATI/AMD videocard drivers",
      path: "old-videocard.ati",
      exit: 1,
      pattern: '^# C  \\[atio(?:gl|[0-9a-z]{2,})xx\\.dll\\+0x[0-9a-z]+\\]$'
    },
    {
      name: "Old Intel videocard",
      path: "old-videocard.intel",
      exit: 1,
      pattern: '^# C  \\[ig4dev[\\d]{2}\\.dll\\+0x[0-9a-z]+]$'
    },
    {
      name: "Too heavy heap",
      path: "reserve-space",
      exit: 1,
      pattern: '^(?:Could not reserve enough space|Initial heap size set to a larger value than the maximum heap size)$'
    }
  ]
}